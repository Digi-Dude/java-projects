<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<FlowPane fx:id="noteWrap" alignment="TOP_CENTER" prefHeight="595.0" prefWidth="1076.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GuiController.NoteWrap">
   <children>
      <ComboBox fx:id="chooseCategory" prefWidth="150.0">
         <FlowPane.margin>
            <Insets bottom="5.0" top="5.0" />
         </FlowPane.margin>
      </ComboBox>
      <ScrollPane hbarPolicy="NEVER" prefHeight="500.0" prefWidth="1076.0">
         <content>
            <FlowPane fx:id="objArea" maxHeight="1.7976931348623157E308" maxWidth="-Infinity" prefHeight="498.0" prefWidth="1074.0" vgap="10.0" />
         </content>
      </ScrollPane>
      <Button fx:id="addNote" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0" style="-fx-background-color: #ffffff; -fx-background-radius: 50;">
         <graphic>
            <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/free-icon-font-add-3914725.png" />
               </image>
            </ImageView>
         </graphic>
         <FlowPane.margin>
            <Insets top="5.0" />
         </FlowPane.margin>
      </Button>
   </children>
</FlowPane>
